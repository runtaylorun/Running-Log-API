{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-tabs": 0,
        "require-await": 1,
        "curly": 1,
        "default-case": 1,
        "default-case-last": 1,
        "eqeqeq": 1,
        "no-multi-spaces": 1,
        "camelcase": "warn",
        "comma-dangle": 1,
    }
}